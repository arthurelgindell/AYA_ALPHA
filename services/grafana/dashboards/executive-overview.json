{
  "dashboard": {
    "title": "AYA Platform - Executive Overview",
    "uid": "aya-executive",
    "tags": ["aya", "overview"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "System Health",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "up{job=~\"prometheus|grafana|node-alpha|postgres-alpha\"}",
            "legendFormat": "{{job}}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center",
          "orientation": "horizontal",
          "reduceOptions": {
            "calcs": ["last"],
            "fields": "",
            "values": false
          },
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "1", "text": "UP", "color": "green"},
              {"type": "value", "value": "0", "text": "DOWN", "color": "red"}
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "Agent Turbo - Knowledge Base",
        "gridPos": {"h": 4, "w": 3, "x": 6, "y": 0},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_knowledge_entries",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 50, "color": "yellow"},
                {"value": 100, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "GLADIATOR - Attack Patterns",
        "gridPos": {"h": 4, "w": 3, "x": 9, "y": 0},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_gladiator_patterns",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "blue"},
                {"value": 1000, "color": "green"},
                {"value": 10000, "color": "yellow"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Code Audit - Runs",
        "gridPos": {"h": 4, "w": 3, "x": 12, "y": 0},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "sum(aya_code_audit_runs)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 5,
        "type": "stat",
        "title": "Agent Tasks - Total",
        "gridPos": {"h": 4, "w": 3, "x": 15, "y": 0},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "sum(aya_agent_tasks_total)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"],
            "fields": "",
            "values": false
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 6,
        "type": "timeseries",
        "title": "Internet Speed Monitor - Download",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_internet_download_mbps",
            "legendFormat": "Download Speed (Mbps)",
            "refId": "A"
          }
        ],
        "options": {
          "tooltip": {"mode": "single", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom"}
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "auto"
            },
            "color": {"mode": "palette-classic"},
            "unit": "Mbps",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 500, "color": "yellow"},
                {"value": 700, "color": "green"}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Download Speed (Mbps)"},
              "properties": [
                {"id": "color", "value": {"fixedColor": "blue", "mode": "fixed"}}
              ]
            }
          ]
        }
      },
      {
        "id": 7,
        "type": "timeseries",
        "title": "Internet Speed Monitor - Upload & Ping",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_internet_upload_mbps",
            "legendFormat": "Upload Speed (Mbps)",
            "refId": "A"
          },
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_internet_ping_ms",
            "legendFormat": "Ping Latency (ms)",
            "refId": "B",
            "yAxis": 2
          }
        ],
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "list", "placement": "bottom"}
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10
            },
            "color": {"mode": "palette-classic"},
            "unit": "short"
          }
        }
      },
      {
        "id": 8,
        "type": "piechart",
        "title": "Agent Tasks by Status",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 12},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_agent_tasks_total",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "right", "values": ["value", "percent"]},
          "pieType": "pie",
          "tooltip": {"mode": "single"}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 9,
        "type": "piechart",
        "title": "Code Audit Findings by Severity",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 12},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_code_audit_findings",
            "legendFormat": "{{severity}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "right", "values": ["value"]},
          "pieType": "pie",
          "tooltip": {"mode": "single"}
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "mappings": [],
            "color": {"mode": "palette-classic"}
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "CRITICAL"},
              "properties": [{"id": "color", "value": {"fixedColor": "red", "mode": "fixed"}}]
            },
            {
              "matcher": {"id": "byName", "options": "HIGH"},
              "properties": [{"id": "color", "value": {"fixedColor": "orange", "mode": "fixed"}}]
            },
            {
              "matcher": {"id": "byName", "options": "MEDIUM"},
              "properties": [{"id": "color", "value": {"fixedColor": "yellow", "mode": "fixed"}}]
            }
          ]
        }
      },
      {
        "id": 10,
        "type": "gauge",
        "title": "Tailscale Network - ALPHA â†” BETA Latency",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 12},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "tailscale_peer_latency_ms{peer=\"beta\"}",
            "legendFormat": "BETA Latency",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true,
          "orientation": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "min": 0,
            "max": 10,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 2, "color": "yellow"},
                {"value": 5, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "type": "timeseries",
        "title": "PostgreSQL Database Size Growth",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_database_size_bytes{database=\"aya_rag\"}",
            "legendFormat": "aya_rag",
            "refId": "A"
          },
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_database_size_bytes{database=\"n8n_aya\"}",
            "legendFormat": "n8n_aya",
            "refId": "B"
          }
        ],
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "table", "placement": "bottom", "showLegend": true}
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 20
            },
            "color": {"mode": "palette-classic"},
            "unit": "bytes"
          }
        }
      },
      {
        "id": 12,
        "type": "table",
        "title": "System Summary",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_agent_sessions_total",
            "format": "table",
            "instant": true,
            "legendFormat": "Agent Sessions",
            "refId": "A"
          },
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "sum(aya_agent_tasks_total)",
            "format": "table",
            "instant": true,
            "legendFormat": "Agent Tasks",
            "refId": "B"
          },
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_knowledge_entries",
            "format": "table",
            "instant": true,
            "legendFormat": "Knowledge Entries",
            "refId": "C"
          },
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_gladiator_patterns",
            "format": "table",
            "instant": true,
            "legendFormat": "GLADIATOR Patterns",
            "refId": "D"
          },
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "sum(aya_code_audit_runs)",
            "format": "table",
            "instant": true,
            "legendFormat": "Code Audit Runs",
            "refId": "E"
          }
        ],
        "transformations": [
          {
            "id": "merge",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "indexByName": {},
              "renameByName": {}
            }
          }
        ],
        "options": {
          "showHeader": true,
          "footer": {
            "show": false
          }
        }
      }
    ],
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    }
  },
  "overwrite": true
}
