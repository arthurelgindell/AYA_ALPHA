{
  "dashboard": {
    "title": "AYA Platform - Mission Critical Systems",
    "uid": "aya-mission-critical",
    "tags": ["aya", "critical", "landing"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "type": "stat",
        "title": "Agent Landing - Version (MISSION CRITICAL)",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_agent_landing_version",
            "legendFormat": "v{{version}}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center",
          "textMode": "name"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "special", "options": {"match": "true", "result": {"text": "ACTIVE", "color": "green"}}}
            ],
            "color": {"mode": "fixed", "fixedColor": "green"}
          }
        }
      },
      {
        "id": 2,
        "type": "stat",
        "title": "System Scope",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 0},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_agent_landing_version",
            "legendFormat": "{{system_scope}}",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "center",
          "textMode": "name"
        },
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "value": "1", "text": "ALPHA + BETA"}
            ],
            "color": {"mode": "fixed", "fixedColor": "blue"}
          }
        }
      },
      {
        "id": 3,
        "type": "stat",
        "title": "Last Updated",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_agent_landing_age_seconds",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "center"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 0,
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 86400, "color": "yellow"},
                {"value": 604800, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "type": "stat",
        "title": "Total Tables in aya_rag",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_table_count_total",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "blue"},
                {"value": 100, "color": "green"},
                {"value": 150, "color": "yellow"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "type": "bargauge",
        "title": "Agent Turbo Tables - Sizes",
        "gridPos": {"h": 12, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_agent_turbo_tables",
            "legendFormat": "{{table_name}}",
            "refId": "A"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 6,
        "type": "stat",
        "title": "Agent Sessions",
        "gridPos": {"h": 4, "w": 3, "x": 12, "y": 4},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_agent_sessions_total",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 7,
        "type": "stat",
        "title": "Agent Tasks",
        "gridPos": {"h": 4, "w": 3, "x": 15, "y": 4},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "sum(aya_agent_tasks_total)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 8,
        "type": "stat",
        "title": "Knowledge Entries",
        "gridPos": {"h": 4, "w": 3, "x": 18, "y": 4},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_knowledge_entries",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 9,
        "type": "stat",
        "title": "GLADIATOR Patterns",
        "gridPos": {"h": 4, "w": 3, "x": 21, "y": 4},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_gladiator_patterns",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 10,
        "type": "table",
        "title": "System Statistics Summary",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_agent_sessions_total",
            "format": "table",
            "instant": true,
            "legendFormat": "Agent Sessions",
            "refId": "A"
          },
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "sum(aya_agent_tasks_total)",
            "format": "table",
            "instant": true,
            "legendFormat": "Agent Tasks",
            "refId": "B"
          },
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_knowledge_entries",
            "format": "table",
            "instant": true,
            "legendFormat": "Knowledge Entries",
            "refId": "C"
          },
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_gladiator_patterns",
            "format": "table",
            "instant": true,
            "legendFormat": "GLADIATOR Patterns",
            "refId": "D"
          },
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "sum(aya_code_audit_runs)",
            "format": "table",
            "instant": true,
            "legendFormat": "Code Audit Runs",
            "refId": "E"
          },
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_table_count_total",
            "format": "table",
            "instant": true,
            "legendFormat": "Total Tables",
            "refId": "F"
          }
        ],
        "transformations": [
          {
            "id": "seriesToRows",
            "options": {}
          },
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "renameByName": {
                "Metric": "Component",
                "Value": "Count"
              }
            }
          }
        ],
        "options": {
          "showHeader": true,
          "footer": {"show": false}
        }
      },
      {
        "id": 11,
        "type": "timeseries",
        "title": "Database Growth - All Tables",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
        "targets": [
          {
            "datasource": {"type": "prometheus", "uid": "PBFA97CFB590B2093"},
            "expr": "aya_table_count_total",
            "legendFormat": "Total Tables",
            "refId": "A"
          }
        ],
        "options": {
          "tooltip": {"mode": "single"},
          "legend": {"displayMode": "list", "placement": "bottom"}
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 20
            },
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      }
    ]
  },
  "overwrite": true
}